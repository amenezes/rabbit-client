[run]
branch = True
omit = **__init__.py**
       rabbit/__version__.py
       rabbit/migrations/*
       tests/unit/migrations/*

[report]
show_missing = True
sort = Cover
precision = 2
exclude_lines = logging.info
                logging.debug
                logging.warn
                logging.error

[paths]
source = rabbit/

[html]
directory = .

[xml]
output = coverage.xml
