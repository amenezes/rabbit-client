[flake8]
exclude = venv
          __pycache__
          *.pyc
          __init__.py
          setup.py
          rabbit/migrations
          tests/unit/migrations
ignore = E501 # line too long
verbose = 2
doctests = True
format = [%(code)s] %(path)s [%(row)d,%(col)d] %(text)s
show_source = True
statistics = True
count = True
